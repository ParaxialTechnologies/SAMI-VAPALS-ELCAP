#!/bin/sh

ENV="/usr/bin/env"

CP="$ENV cp"
MKDIR="$ENV mkdir"
RM="$ENV rm"

REPO="$HOME/lib/silver/a-sami-vapals-elcap--vo-osehra-github"
INDIR="$HOME/run/in/sami/18.13/t1"

cd $INDIR
$MKDIR -p ./back
printf "Backing up files ... "
$CP -r $REPO/docs/form-fields ./back/
$CP -r $REPO/docs/www ./back/
echo "done."

